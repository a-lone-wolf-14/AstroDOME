{
  "headers": [
    {
      "source": "/(.*)\\.(webm|mp4)",
      "headers": [
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
